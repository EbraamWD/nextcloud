version: "3.7"
services:
  app:
    image: nextcloud
    command: 
             --transaction-isolation=READ-COMMITTED --binlog-               format=ROW
    ports: 
          - 8080:80
    volumes: 
          - nextcloud:/var/www/html
    environment:
      - MYSQL_PASSWORD=
      - MYSQL_DATABASE=nextcloud
      - MYSQL_USER=nextcloud
      - MYSQL_HOST=db

